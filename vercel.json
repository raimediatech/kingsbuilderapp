{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/builder/120003199218",
      "dest": "/public/builder/120003199218.html",
      "headers": {
        "Content-Security-Policy": "frame-ancestors 'self' https://*.myshopify.com https://*.shopify.com; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://cdn.shopify.com https://cdnjs.cloudflare.com;",
        "X-Frame-Options": "ALLOWALL"
      }
    },
    {
      "src": "/builder/120147771634",
      "dest": "/public/builder/120147771634.html",
      "headers": {
        "Content-Security-Policy": "frame-ancestors 'self' https://*.myshopify.com https://*.shopify.com; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://cdn.shopify.com https://cdnjs.cloudflare.com;",
        "X-Frame-Options": "ALLOWALL"
      }
    },
    {
      "src": "/builder/(.*)",
      "dest": "/public/builder/builder.html",
      "headers": {
        "Content-Security-Policy": "frame-ancestors 'self' https://*.myshopify.com https://*.shopify.com; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://cdn.shopify.com https://cdnjs.cloudflare.com;",
        "X-Frame-Options": "ALLOWALL"
      }
    },
    {
      "src": "/app",
      "dest": "/api/index.js"
    },
    {
      "src": "/dashboard",
      "dest": "/api/index.js"
    },
    {
      "src": "/dashboard/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/pages",
      "dest": "/api/index.js"
    },
    {
      "src": "/pages/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/templates",
      "dest": "/api/index.js"
    },
    {
      "src": "/templates/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/settings",
      "dest": "/api/index.js"
    },
    {
      "src": "/settings/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/help",
      "dest": "/api/index.js"
    },
    {
      "src": "/help/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/pages",
      "dest": "/api/index.js"
    },
    {
      "src": "/install",
      "dest": "/public/install.html"
    },
    {
      "src": "/editor",
      "dest": "/public/editor.html"
    },
    {
      "src": "/analytics",
      "dest": "/public/analytics.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}