{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [

    {
      "src": "/app",
      "dest": "/api/index.js"
    },
    {
      "src": "/dashboard",
      "dest": "/api/index.js"
    },
    {
      "src": "/dashboard/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/pages",
      "dest": "/api/index.js"
    },
    {
      "src": "/pages/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/templates",
      "dest": "/api/index.js"
    },
    {
      "src": "/templates/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/settings",
      "dest": "/api/index.js"
    },
    {
      "src": "/settings/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/help",
      "dest": "/api/index.js"
    },
    {
      "src": "/help/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/pages",
      "dest": "/api/index.js"
    },
    {
      "src": "/install",
      "dest": "/public/install.html"
    },


    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}